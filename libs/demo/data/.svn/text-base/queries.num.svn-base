select * from num;
select * from num where 1+1=2;
select a+b+c+d+e from num;
select * from num where a = 1 or e = 1;
select a, b+e from num where a = 1 and b = 1 or e = 1;
select * from num where a + b + c <= 3;
select a ** 2 from num where 3 = a + b + c;
select * from num where a = b or e = d - 1;
select * from num where b ** a <= 2;
select a, b from num where a not between 1 and 2;
select a, b from num where a not in (1, 2);
select a, b from num where a**2 in (1, 4);
exit;
